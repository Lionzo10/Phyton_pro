import random
def gen(clave):
    teclas = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
    contra = "".join(random.choice(teclas) for i in range(clave))
    print("La clave generada es:", contra)
    return contra
claveaa= int(input("Longitud clave: "))
clavegen= gen(claveaa) 
